volume = {35}, 
month = {Sep},
year = {2010},
url = {http://ol.osa.org/abstract.cfm?URI=ol-35-18-3102},
doi = {10.1364/OL.35.003102},
abstract = {We demonstrate the capabilities of a new optical microelectromechanical systems device that we specifically developed for broadband femtosecond pulse shaping. It consists of a one-dimensional array of 100 independently addressable, high-aspect-ratio micromirrors with up to 3$\mu$m stroke. We apply linear and quadratic phase modulations demonstrating the temporal compression of 800 and 400nm pulses. Because of the device's surface flatness, stroke, and stroke resolution, phase shaping over an unprecedented bandwidth is attainable.},
}

@article{Maksimenka:10, 
author = {Raman Maksimenka and Patrick Nuernberger and Kevin F. Lee and Adeline Bonvalet and Jadwiga Milkiewicz and Cestmir Barta and Milo\v{s} Klima and Thomas Oksenhendler and Pierre Tournois and Daniel Kaplan and Manuel Joffre}, 
journal = {Opt. Lett.}, 
keywords = {Acousto-optical materials; Acousto-optical devices; Pulse shaping},
number = {21}, 
pages = {3565--3567}, 
publisher = {OSA},
title = {Direct mid-infrared femtosecond pulse shaping with a calomel acousto-optic programmable dispersive filter}, 
volume = {35}, 
month = {Nov},
year = {2010},
url = {http://ol.osa.org/abstract.cfm?URI=ol-35-21-3565},
doi = {10.1364/OL.35.003565},
abstract = {Direct amplitude and phase shaping of mid-infrared femtosecond pulses is realized with a calomel-based acousto-optic programmable dispersive filter transparent between 0.4 and 20$\mu$m. The shaped pulse electric field is fully characterized with high accuracy, using chirped-pulse upconversion and time-encoded arrangement spectral phase interferometry for direct electric field reconstruction techniques. Complex mid-infrared pulse shapes at a center wavelength of 4.9$\mu$m are generated with a spectral resolution of 14cm{\textminus}1, which exceeds by a factor of 5 the reported experimental resolutions of calomel-based filters.},
}

@article{Seidel:11, 

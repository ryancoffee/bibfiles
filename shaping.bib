volume = {12}, 
month = {Jun},
year = {2016},
url = {http://jdt.osa.org/abstract.cfm?URI=jdt-12-6-589},
abstract = {Light beams shaped by programmable megapixel spatial light modulators(SLMs) are key to broadening the applications of photonics. In thispaper, we consider the application of a SLM for the generation oftwo mutually coherent white-light continuum optical sources by filamentationof infrared femtosecond pulses in bulk. We demonstrate that the inhomogeneityof the input beam and the longitudinal separation of the generatedfilaments are crucial parameters that break down the mutual coherenceacross neighboring filaments. We show that local control over theoptical phase enables us to gain fine control over filament interferenceeffects.},
}

@article{Rahimi:16, 
